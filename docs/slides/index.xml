&lt;div class="highlight">&lt;pre tabindex="0" class="chroma">&lt;code class="language-MATLAB" data-lang="MATLAB">&lt;span class="line">&lt;span class="cl">&lt;span class="c">% 4 bit exponent (signed)&lt;/span>
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="n">emin&lt;/span> &lt;span class="p">=&lt;/span> &lt;span class="o">-&lt;/span>&lt;span class="mi">7&lt;/span>&lt;span class="p">;&lt;/span> &lt;span class="n">emax&lt;/span> &lt;span class="p">=&lt;/span> &lt;span class="mi">6&lt;/span>&lt;span class="p">;&lt;/span>
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="c">% 3 bits significand&lt;/span>
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="n">p&lt;/span> &lt;span class="p">=&lt;/span> &lt;span class="mi">3&lt;/span>&lt;span class="p">;&lt;/span>
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="n">exps&lt;/span> &lt;span class="p">=&lt;/span> &lt;span class="mf">2.&lt;/span>^&lt;span class="p">[&lt;/span>&lt;span class="n">emin&lt;/span>&lt;span class="p">:&lt;/span>&lt;span class="n">emax&lt;/span>&lt;span class="p">];&lt;/span>
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="n">vals&lt;/span> &lt;span class="p">=&lt;/span> &lt;span class="p">(&lt;/span>&lt;span class="mi">2&lt;/span>^&lt;span class="n">p&lt;/span>&lt;span class="p">)&lt;/span> &lt;span class="o">+&lt;/span> &lt;span class="p">[&lt;/span>&lt;span class="mi">0&lt;/span>&lt;span class="p">:(&lt;/span>&lt;span class="mi">2&lt;/span>^&lt;span class="n">p&lt;/span>&lt;span class="o">-&lt;/span>&lt;span class="mi">1&lt;/span>&lt;span class="p">)];&lt;/span> &lt;span class="c">% significand with leading 1&lt;/span>
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="n">TotalVal&lt;/span> &lt;span class="p">=&lt;/span> &lt;span class="n">vals&lt;/span>&lt;span class="o">&amp;#39;*&lt;/span>&lt;span class="n">exps&lt;/span>&lt;span class="p">;&lt;/span>
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="c">% add denormalized values&lt;/span>
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="mi">8&lt;/span>&lt;span class="n">bitFloat&lt;/span> &lt;span class="p">=&lt;/span> &lt;span class="n">unique&lt;/span>&lt;span class="p">([&lt;/span>&lt;span class="n">TotalVal&lt;/span> &lt;span class="p">;&lt;/span> &lt;span class="mi">2&lt;/span>^&lt;span class="n">emin&lt;/span> &lt;span class="o">*&lt;/span> &lt;span class="p">[&lt;/span>&lt;span class="mi">0&lt;/span>&lt;span class="p">:(&lt;/span>&lt;span class="mi">2&lt;/span>^&lt;span class="n">m&lt;/span>&lt;span class="o">-&lt;/span>&lt;span class="mi">1&lt;/span>&lt;span class="p">)]&lt;/span>&lt;span class="o">&amp;#39;&lt;/span>&lt;span class="p">]);&lt;/span>
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="mi">8&lt;/span>&lt;span class="n">bitFloat&lt;/span> &lt;span class="p">=&lt;/span> &lt;span class="n">unique&lt;/span>&lt;span class="p">(&lt;/span>&lt;span class="nb">flipud&lt;/span>&lt;span class="p">(&lt;/span>&lt;span class="o">-&lt;/span>&lt;span class="n">TotalVal&lt;/span>&lt;span class="p">)&lt;/span> &lt;span class="p">;&lt;/span>&lt;span class="n">TotalVal&lt;/span>&lt;span class="p">]);&lt;/span> &lt;span class="c">% with neg vals&lt;/span>
&lt;/span>&lt;/span>&lt;/code>&lt;/pre>&lt;/div>&lt;hr>
